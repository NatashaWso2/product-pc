<%
importPackage(org.wso2.carbon.pc.core);

var log = new Log();
if(log.isDebugEnabled()){
    log.debug("INSIDE get process variables list...");
}

var ps = new ProcessStore();

var processResourcePath = String(request.getParameter("process_resource_path"));
if(log.isDebugEnabled()){
    log.debug("Retrieving process variables for " + processResourcePath);
}
var processVariablesList = ps.getProcessVariablesList(processResourcePath);
print(processVariablesList);

%>