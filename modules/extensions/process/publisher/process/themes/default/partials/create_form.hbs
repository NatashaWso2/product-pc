    {{#with assets}}
<form  method='post' action='{{url ""}}/apis/assets?type={{../rxt.shortName}}' class="form-horizontal" id="form-asset-create" data-redirect-url='{{url ""}}/asts/{{../rxt.shortName}}/list' >
    <div class="control-group">
        <div id = "headers"><table>
            <tbody>
                
                <tr>
                        <input type="text" placeholder="Enter Process Name" name="overview_name" id="processName" class="input-large" style="width:785px";>
                </tr>
                <tr>
                   <td> <input type="text" placeholder="Enter Provider Name" name="overview_provider" class="input-large" style="width:425px";></td>
                
                   <td> <input type="text" placeholder="Enter Verision" name="overview_version" class="input-large" style="width:343px";></td>
                </tr>
            </tbody></table></div>
            <hr width="800px" />
            <div style="padding-bottom:15px"> <textarea placeholder="Enter Process Details"rows = "15" id ="processContent" name = "processContent" class="input-large" style = "width:785px"></textarea></div>
            <div id= "proptable">
                <table id="htmlgrid" class="testgrid">
                    <tr>
                        <th>Properties</th>
                        <th>Value</th>
                    </tr>
                    
                    <tr id="R1">
                        <td>Predecessors</td>
                        <td><input type="text"  value="" id="properties_predecessors" name="properties_predecessors" style="border:none;border-color:transparent;"></td>
                    </tr>
                    <tr id="R2">
                        <td>Successors</td>
                        <td><input type="text" value="" id="properties_successors"name="properties_successors" style="border:none;border-color:transparent;" ></td>
                    </tr>
                    <tr id="R3">
                        <td>Generalizations</td>
                        <td><input type="text" value="" id="properties_generalizations"name="properties_generalizations" style="border:none;border-color:transparent;"></td>
                    </tr>
                    <tr id="R4">
                        <td>Specialization</td>
                        <td><input type="text" value="" id="properties_specializations" name="properties_specializations" style="border:none;border-color:transparent;" ></td>
                    </tr>
                </table>
            </div>
            <hr width="800px" />
            <div id="images">
                <table>
                    <tbody><tr>
                        <td><b><label class="control-label">Thumbnail</label></b></td><td><input type="file" value="" name="images_thumbnail" class="input-large"></td>
                    </tr>
                </tbody></table>
            </div>
        </div>
        <div id="saveButtons" class="form-actions">
            <input type='submit' id="btn-create-asset" class="btn btn-primary" name="addNewAssetButton" value='{{t "Create"}}'>
            <button class="btn" type="reset">{{t "Reset"}}</button>
            <input type="hidden" value="{{rxt.shortName}}" name="{{rxt.shortName}}" id="meta-asset-type">
        </div>
    </form>
    {{/with}}