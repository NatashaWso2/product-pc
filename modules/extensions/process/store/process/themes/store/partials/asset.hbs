<script>
    var isSocial = ('{{features.social.enabled}}' == 'true');
</script>
<div class="asset-description">
    {{#with assets}}
        {{#with attributes}}
            <div class="asset-description-header">
                <div class="row">
                    <div class="span9" style="height:auto">
                        <div>
                            <div id="processName">
                                <h3>{{overview_name}}</h3>
                            </div>
                            <small> {{t "Version"}} {{overview_version}}</small>
                            <small> {{t "by"}} {{overview_provider}} </small>
                            <span class="asset-rating">
                            <div style="width:{{../ratingPx}}px"></div>
                            </span>
                            <hr width="800">
                            <div id="content" class="content"
                                 style="text-align:justify; width: 800px ">
                            </div>
                            <div name="propTable">
                                <hr width="800">
                                <table id="htmlgrid" class="testgrid">
                                    <tr>
                                        <th>Properties</th>
                                        <th>Value</th>
                                    </tr>
                                    <tr id="R1">
                                        <td>Predecessors</td>
                                        <td id="td_pre">{{properties_predecessors}}</td>
                                    </tr>
                                    <tr id="R2">
                                        <td>Successors</td>
                                        <td id="td_suc">{{properties_successors}}</td>
                                    </tr>
                                    <tr id="R3">
                                        <td>Generalizations</td>
                                        <td id="td_gen">{{properties_generalizations}}</td>
                                    </tr>
                                    <tr id="R4">
                                        <td>Specialization</td>
                                        <td id="td_spec">{{properties_specialization}}</td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="assetp-properties">
                <ul class="nav nav-tabs" id="assetp-tabs" data-aid="{{../path}}">
                    <li class="active">
                        <a href="#tab-reviews" data-toggle="tab"
                           data-type="comments">{{t "User Reviews"}}</a>
                    </li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane active" id="tab-reviews">
                        {{#if ../../features.social.enabled}}
                            <iframe name="socialIfr" id="socialIfr"
                                    src="{{../../../features.social.keys.socialAppUrl}}?target={{../../../rxt.shortName}}:{{../../id}}&user={{../../../cuser.username}}"
                                    width="100%" height="100%" style="height: 300px;"
                                    data-script="{{../../../features.social.keys.socialScriptSource}}"
                                    data-script-type="{{../../../features.social.keys.socialScriptType}}"></iframe>
                        {{/if}}
                        {{#if ../../inDashboard}}
                            <div id="comment-content" class="content"></div>
                        {{else}}
                            <div id="comment-content" class="content user-review"></div>
                        {{/if}}
                    </div>
                </div>
            </div>
        {{/with}}
    {{/with}}
</div>
